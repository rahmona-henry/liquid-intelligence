    "url": "https://github.com/rahmona-henry/liquid-intelligence/issues"
  },
  "homepage": "https://github.com/rahmona-henry/liquid-intelligence#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.0",
    "connect-session-knex": "^1.3.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "handlebars": "^4.0.6",
    "hbs": "^4.0.0",
    "hbsfy": "^2.7.0",
    "jquery": "^3.1.0",
    "knex": "^0.12.8",
    "path": "^0.12.7",
    "pg": "^6.1.2",
