{
  "name": "liquid-intelligence",
  "version": "1.0.0",
  "description": "An online repository for my nano brewery recipes.",
  "engine": {
    "node": "6.9.2"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./server.js",
    "build": "browserify -t hbsfy ./src/index.js -g uglifyify > ./client/app.js",
    "serve": "node-dev server.js",
    "watch": "watchify -t hbsfy ./src/index.js -o ./client/app.js -dev",
    "postinstall": "browserify -t hbsfy ./src/index.js -g uglifyify > ./client/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahmona-henry/liquid-intelligence.git"
  },
